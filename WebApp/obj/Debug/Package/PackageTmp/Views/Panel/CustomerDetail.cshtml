@model WebApp.Models.CustomerCompleteModel

<div class="container-fluid" style="text-align: center; margin-top: 15px">
    <div class="row">
        <div class="col-xs-4">
            <a href="@Url.Action("Index", "Panel")">
                <img src="~/Images/appbar.arrow.left.png" style="width: 60px; height: 60px" /></a>
        </div>
        <div class="col-xs-4">
            <span style="color: white; font-size: large">Detalle de usuario</span>
        </div>
        <div class="col-xs-4">
        </div>
    </div>
    <div class="row" style="margin-top: 15px">
        <div style="color: white; font-size: medium">
            Nombre: @Html.DisplayFor(m => m.Name) @Html.DisplayFor(m => m.LastName)
        </div>
    </div>
    <div class="row">
        <div style="color: white; font-size: medium">
            E-mail: @Html.DisplayFor(m => m.Email)
        </div>
    </div>
    <div class="row">
        <div style="color: white; font-size: medium">
            Telefono: @Html.DisplayFor(m => m.Phone) Movil: @Html.DisplayFor(m => m.Mobile)
        </div>
    </div>
    <div class="row">
        <div style="color: white; font-size: medium">
            Direccion: @Html.DisplayFor(m => m.Address) @Html.DisplayFor(m => m.ExtNum) @Html.DisplayFor(m => m.IntNum)
        </div>
    </div>
    <div class="row">
        <div style="color: white; font-size: medium">
            Colonia: @Html.DisplayFor(m => m.ResidentialArea)
        </div>
    </div>
    <div class="row">
        <div style="color: white; font-size: medium">
            @Html.DisplayFor(m => m.City), @Html.DisplayFor(m => m.State)
        </div>
    </div>
    <div class="row">
        <div style="color: white; font-size: medium">
            Codigo Postal: @Html.DisplayFor(m => m.ZipCode)
        </div>
    </div>
</div>
